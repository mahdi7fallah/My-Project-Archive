<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Toplevel" id="mainwindow">
    <property name="background">#3f3f3f</property>
    <property name="borderwidth">5</property>
    <property name="cursor">arrow</property>
    <property name="height">160</property>
    <property name="highlightbackground">#d9d9d9</property>
    <property name="highlightcolor">#d9d9d9</property>
    <property name="maxsize">205|160</property>
    <property name="minsize">205|160</property>
    <property name="padx">3</property>
    <property name="pady">1</property>
    <property name="relief">flat</property>
    <property name="resizable">none</property>
    <property name="title" translatable="yes">pygubu01</property>
    <property name="width">200</property>
    <child>
      <object class="tk.Label" id="lbl1">
        <property name="background">#3f3f3f</property>
        <property name="foreground">#ffffff</property>
        <property name="text" translatable="yes">Poten 1 Value:</property>
        <property name="width">15</property>
        <layout>
          <property name="column">1</property>
          <property name="padx">2</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="lbl_poten1_value">
        <property name="background">#3f3f3f</property>
        <property name="foreground">#ffffff</property>
        <property name="text" translatable="yes">Value</property>
        <property name="width">5</property>
        <layout>
          <property name="column">2</property>
          <property name="padx">5</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="btn_on_off">
        <property name="background">#3f3f3f</property>
        <property name="compound">top</property>
        <property name="foreground">#ffffff</property>
        <property name="text" translatable="yes">ON / OFF</property>
        <property name="width">9</property>
        <layout>
          <property name="column">1</property>
          <property name="padx">2</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="lbl_on_off">
        <property name="background">#3f3f3f</property>
        <property name="foreground">#ffffff</property>
        <property name="text" translatable="yes">Status</property>
        <property name="width">5</property>
        <layout>
          <property name="column">2</property>
          <property name="padx">5</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Scale" id="sld_motor1">
        <property name="background">#3f3f3f</property>
        <property name="foreground">#ffffff</property>
        <property name="from_">0</property>
        <property name="label" translatable="yes">Motor 1 Value</property>
        <property name="length">160</property>
        <property name="orient">horizontal</property>
        <property name="resolution">1</property>
        <property name="showvalue">true</property>
        <property name="sliderlength">15</property>
        <property name="sliderrelief">flat</property>
        <property name="tickinterval">0</property>
        <property name="to">10</property>
        <property name="width">15</property>
        <layout>
          <property name="column">1</property>
          <property name="columnspan">10</property>
          <property name="padx">14</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
